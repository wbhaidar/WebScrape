		//} ZD#88040
		
		function showFares (pName) {
			var e = $(".event_fare");
			for (var i = 0; i < e.length; i++) {
				if ($(e[i]).hasClass(pName)) {
					e[i].style.display = "block";
				} else {
					e[i].style.display = "none";
					e[i].html = "";
				}
			}
		}
		
		var currentFare;
		var currentRoom;
		var currentIncrement;
		var validPage = "OK";
		
		function updateFareQty(key, fare, roomType, increment) {
			currentFare = fare;
			currentRoom = roomType;
			currentIncrement = increment;
			var submitData = "key=" + key + "&fare=" + fare + "&roomtype=" + roomType + "&increment=" + increment;
			ssAjaxRequest("updateFareQty", submitData, "", actionUpdateFareQtyResult);
		}
		
		function updateFareQtyNew(key, fare, roomType, t) {
			currentFare = fare;
			currentRoom = roomType;
			currentIncrement = 0;
			var submitData = "key=" + key + "&fare=" + fare + "&roomtype=" + roomType + "&newQty=" + $(t).val();
			ssAjaxRequest("updateFareQty", submitData, "", actionUpdateFareQtyResult);
		}
		
		function actionUpdateFareQtyResult(data, textStatus, jqXHR){
			var result = $.parseJSON(data);
			
			if (result.result == "OK"){
			//it worked
				$(".cl_be__cart").html(result.htmlCart);	
				$(".cartFooter").html(result.htmlCartFooter);
				setValid(result.valid);
			} else {
				if(result.newValue){
					$(".fare" + currentFare + currentRoom).val(result.newValue);
				} else {
					if (currentIncrement == 0) {
						$(".fare" + currentFare + currentRoom).val(0);
					} else {
						$(".fare" + currentFare + currentRoom).val($(".fare" + currentFare + currentRoom).val() - currentIncrement);
					}
				}
			}
		}
		
		function validatePage(){
			var _return = true;
			if (validPage.length > 0) {
				showInfoBar(validPage, true, true);
				_return = false;
			}
			return _return;
		}
