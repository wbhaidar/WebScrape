										processAvailability(result.availability);
									});
									
									function processAvailability(v) {
										var arr = []
										for (var av of v) 
											{
												if (av.status != "Available") {
													arr.push(new Date(av.date.split("/").reverse().join("/")));
												}
												
											  //document.write(av.date + " " + av.status + "<br />");
											}
										$("#datetimepicker-").datetimepicker("disabledDates", arr);
										$(".day").each(function(index) {
											var day = $(this).attr("data-day");
											var found = v.find(e => e.date === day)
											if (found != undefined) {
												$(this).attr("title", found.status);
											}
										});
										
									}
								  </script>
							</div>
							<a onclick="javascript:changeDate(this.getAttribute('data-mydate'), 1)" data-mydate="19/12/2024" class="date-right-arrow cl_availability__right-small-screen ml-1" style="cursor: pointer;"><svg viewBox="0 0 24 24" style="width: 24px; height: 24px;"><path fill="currentColor" d="M22,12A10,10 0 0,1 12,22A10,10 0 0,1 2,12A10,10 0 0,1 12,2A10,10 0 0,1 22,12M20,12A8,8 0 0,0 12,4A8,8 0 0,0 4,12A8,8 0 0,0 12,20A8,8 0 0,0 20,12M8.6,16.6L13.2,12L8.6,7.4L10,6L16,12L10,18L8.6,16.6Z"></path></svg></a>

						</div>
					</div>
				</div>
	
