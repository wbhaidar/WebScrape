			}
		}
		function getInteval(vValue) {
			return vValue * $(".cl_availability-header_circle:visible").length;
		}
		function selectDate(newSelectedDate) {
			var submitData = "newDate=" + newSelectedDate;
			ssAjaxRequest("selectDate", submitData, "", actionChangeDateResult);
		}
	
		var changeDateDate;
		function changeDate( newDate, direction) {		
			var submitData = "newDate=" + newDate + "&direction=" + direction;
			ssAjaxRequest("changeDate", submitData, "", actionChangeDateResult);
		}
				
		function updateDate(newDate) {
			var submitData = "newDate=" + newDate;
			ssAjaxRequest("updateDate", submitData, "", actionUpdateResult);
		}
		
		function actionUpdateResult(data, textStatus, jqXHR){
			var result = $.parseJSON(data);
			
			if (result.result == "OK"){
			//it worked			
				if (result.availability) {processAvailability(result.availability);}	
				setValid(result.valid);					
			}
		}	
		
		// ZD#89034
		function changeGroupSize() {
			var submitData = "&groupSize=" + $(".groupSize").val();
			ssAjaxRequest("changeGroupSize", submitData, "", actionChangeGroupSize);
		}
		
		function actionChangeGroupSize(data, textStatus, jqXHR){
			var result = $.parseJSON(data);
			if(result.newValue){
				$(".groupSize").val(result.newValue);
			}
			if (result.result == "OK"){
			//it worked	
				$("#AvailabilityTable").html(result.availabilityHTML);	
                if (result.cartFooterHTML) {$("#divContentCartFooter").html(result.cartFooterHTML);}
				setValid(result.valid);					
			} 
		}
		
		function setValid(value) {
			validPage = value;
			
			$(".cart--footer--btns>.btn--action").removeClass("disabled");
			if (validPage != "") {
				$(".cart--footer--btns>.btn--action").addClass("disabled");
			}
		}
		
		function setProduct(ProductId, newDate) {		
			var submitData = "ProductId=" + ProductId + "&newDate=" + newDate;
			ssAjaxRequest("SetProduct", submitData, "", actionChangeScheduledEventResult);
		}
		
		function validatePageNoMessage(){
			var _return = true;
			if (validPage.length > 0) {
				//showInfoBar(validPage, true, true);
				_return = false;
			}
			return _return;
		}
		
		function updateScheduledEvent() {	
			if (typeof validatePage === "function") { 
				if (!validatePageNoMessage()) {return false;}
			}
			
			var submitData = "confirm=naKlikEK1";
			ssAjaxRequest("UpdateProduct", submitData, "", actionChangeScheduledEventResult);
		}

		function changeScheduledEvent(ProductId, newDate) {		
			var submitData = "ProductId=" + ProductId + "&newDate=" + newDate;
			ssAjaxRequest("SelectProduct", submitData, "", actionChangeScheduledEventResult);
		}
		
		function actionChangeScheduledEventResult(data, textStatus, jqXHR){
				var result = $.parseJSON(data);
				
				if (result.result == "OK"){
					if (result.urlNewTab) {
						//window.open(result.urlNewTab, "_blank");
						//alert(result.urlNewTab);
						var bUrl = result.urlNewTab;
						
						window.parent.postMessage(
						    {
							event_id: 'Message',
							data: {url: bUrl}
							}, 
							"*"
						);
					}
					//location.href = result.url;
				} 
				
		}
		
