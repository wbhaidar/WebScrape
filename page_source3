<script>
jQuery(document).ready(function (e) {
	function t(t) {
		e(t).bind("click", function (t) {
		  t.preventDefault();
		  e(this).parent().fadeOut()
		})
	}

	e(".partyDetails-dropdown-toggle-down").click(function(){
		e(".partyDetails-dropdown-menu").toggle();
	})

	e(".partyDetails-dropdown-toggle-up").click(function(){
		e(".partyDetails-dropdown-menu").toggle();
	})

	 e(document).on("click", ".dropdown-toggle-menu", function (t) {
		var t = e(this).parents(".button-dropdown").children(".dropdown-menu").is(":hidden");
		e(".button-dropdown .dropdown-menu").hide();
		e(".button-dropdown .dropdown-toggle1").removeClass("active");
		if (t) {
			e(this).parents(".button-dropdown").children(".dropdown-menu").toggle().parents(".button-dropdown").children(".dropdown-toggle1").addClass("active")
		}
	});
	e(document).bind("click", function (t) {
		var n = e(t.target);
		if (!n.parents().hasClass("button-dropdown")) e(".button-dropdown .dropdown-menu").hide();
	});
	e(document).bind("click", function (t) {
		var n = e(t.target);
		if (!n.parents().hasClass("button-dropdown")) e(".button-dropdown .dropdown-toggle1").removeClass("active");
	})
   setTimeout(doSessionCheck, 60000);

});

var gCallbacks = [];
function currentTime(incMilliSeconds){
	var d = new Date();
	return dateYYYYMMDD(d)+', '+timeHHMMSS(d, incMilliSeconds);
}

function timeHHMMSS(d, incMilliSeconds, delimiter){
    if (isNaN(d.getFullYear())) return "";

    var hrs = d.getHours();
    var mns = d.getMinutes();
    var scs = d.getSeconds();
		var mls	= d.getMilliseconds();
		if (incMilliSeconds) {
			if (delimiter) return [hrs, mns, scs, mls].join(delimiter);
			else return [hrs, mns, scs, mls].join(':');
			
		} else {
			if (delimiter) return [hrs, mns, scs].join(delimiter);
			else return [hrs, mns, scs].join(':');
		}
}

function dateYYYYMMDD(d){
    if (isNaN(d.getFullYear())) return "";

    month = '' + (d.getMonth() + 1);
    day = '' + d.getDate();
    year = d.getFullYear();

    if (month.length < 2) month = '0' + month;
    if (day.length < 2) day = '0' + day;

    return [year, month, day].join('-');
}

function localTime(){
	//returns the local time of the browser at the time the request is made - used to add to ajax requests
	return '&localtime='+ currentTime(true);
}

//Add Action to the tag
function ssAjaxRequest(ajaxFunction, ajaxData, idTagToPopulate, successFunction){
//if an idTagToPopulate is given it will show content in there
//otherwise it will run the function given with the reponse details passed to it
	
	
	
	var processResult = true;
	$.ajax({method: "POST", 
		url: jadeHTTP+'&'+ajaxFunction+localTime(),
		data: ajaxData,
		cache: false,
		async: false,
        headers: {
           "cache-control": "no-cache"
        },		
		success: function(data, textStatus, jqXHR){  		
			if (jqXHR.readyState == 4 && jqXHR.status == 200) {	
				checkResultForLogout(data);
				if (successFunction && successFunction != ssJSONResult){
				//don't check this stuff if our success function is already ssJSONResult
					var result = ssJSONResult(data, textStatus, jqXHR);
					processResult = !result || result.result == "OK";
				}
				if (processResult && idTagToPopulate != "") {
					if (jqXHR.responseText.charAt(0) == "{"){
					//we were expecting html but we have json back - probably an error
						ssJSONResult(data, textStatus, jqXHR);
						return;
					} else {
					//populate the tag given
						$("#"+idTagToPopulate).html(jqXHR.responseText);
					}
				}
				if (successFunction && typeof(successFunction) == 'function') {
					successFunction(data, textStatus, jqXHR); 
				}
			} else {
				alertError(jqXHR, textStatus, data);
			}
		},
		error: function(jqXHR, textStatus, errorThrown){
			alertError(jqXHR, textStatus, errorThrown);
		}
	});
}
			
//ZD#94058
var sessionTimedout = false;
function checkResultForLogout(result) {
    if (sessionTimedout == false) {
        var title = "Session Expired";
        var body = "Your session has expired";
        var button = "Start again";
        var message = "<div style='text-align:center;max-height:calc(100vh - 170px);overflow:auto; padding-right:5px;padding-bottom:1rem'><i class='far fa-clock display-6' style='color:var(--btn-action-bg-color);'></i><p>" + unescape(body) + "</p></div>"
        if (result == "logout") {
            bootBoxModal = bootbox.dialog({
              message: message,
              title: title,
              closeButton:false,
              swapButtonOrder:true,
              buttons: {
                confirm: {
                  label: button,
                  className: "btn--action",
                    callback: restartSession
                }
                }
            });
            sessionTimedout = true;
        }
    }
}

    function restartSession() {            
                                 
            location.href = "/puffingbillyrailways/BookingCat/Availability/?" ;
    }    
     
    


function alertError(jqXHR, textStatus, errorThrown) {
	if ($.trim(errorThrown) == "" || $.trim(errorThrown) == "--" ) return;
	
	var response = "";
	var textToShow = "";
	if (jqXHR) response = '-<br>Response: '+jqXHR.responseText;		
	textToShow = '<p>There was unfortunately an error trying to retrieve your data.</p><p><i>Error: -'+errorThrown+response+'</i></p>';
	
	if (errorThrown == 'Service Unavailable')
		textToShow += '<p><b>Note: </b>A "service is unavailable" error either means that your wifi or mobile data is experiencing difficulties or that we are performing systems maintenance. Please try again in 10-15 minutes.</p>'
	
	showInfoBar(textToShow, true, true);
}

function ssJSONResult(data, textStatus, jqXHR){
    //expects JSON structure to have a result: then these are optional errormessage: successmessage: successinfo: htmls
	try {
		var result = jQuery.parseJSON(data);
	} catch(error) {
		return;
	}

    if (result.result && result.result != "OK" && result.result != "CONFIRM" && result.result != "HTML" && result.result != "Error") showInfoBar(result.result, false, true);
	if (result.htmlCart) $(".cl_be__cart").html(result.htmlCart);
	if (result.htmlCartFooter) $(".cartFooter").html(result.htmlCartFooter);
	if (result.htmlContentCartFooter) $("#divContentCartFooter").html(result.htmlContentCartFooter);
	
	if (result.htmls){
		var lHtmlObj = {};
		for (lInd = 0; lInd < result.htmls.length; lInd++) {
			lHtmlObj = result.htmls[lInd];
			$(lHtmlObj.htmlName).html(lHtmlObj.htmlContent);
		}
	}
	if (result.successAction){
		try {
			window[result.successAction];
		} catch(error) {
			return;
		}
	}
	if (result.successmessage) {
		showModal("Success", unescape(result.successmessage));
	}
	if (result.successinfo) showInfoBar(result.successinfo, false, false);
	if (result.script) jQuery.globalEval(decodeURI(result.script));
	if (result.url) location.href = result.url;
	if (result.hasOwnProperty("valid")) validPage = result.valid;
	if (result.errormessage) {
			showModal("Error", translateErrorMessage(unescape(result.errormessage)));
	}
    if (result.errorinfo) showInfoBar(result.errorinfo, false, true);
	if (result.scrollTo){
		//$(result.scrollTo + ":first").scrollIntoView();
		//$(result.scrollTo).get(0).scrollTop(300);
		var vScrollTo = document.querySelector(result.scrollTo);
		vScrollTo.scrollIntoView();
		}

	if (result.confirm){
		var lConfirm = {};
		lConfirm = result.confirm;
		var lFunctionYes = gCallbacks[lConfirm.functionYes];
		var lFunctionNo = gCallbacks[lConfirm.functionNo];
		if (lFunctionYes == null) {
			showModal(lConfirm.title, lConfirm.body, lFunctionNo);
		} else {
			showModalConfirm(lConfirm.title, lConfirm.body, lFunctionYes, lFunctionNo, lConfirm.labelYes, lConfirm.labelNo);
		}
	}

	if (result.refreshPageForCheckout) {
		showModal("Warning", result.refreshPageForCheckout, refreshPageForCheckout);
	}

    return result;
}

function refreshPageForCheckout() {
	location.href = location.href;
}

function translateErrorMessage(vError) {
	var replaceError = vError.includes("OVERLAND Booking exceeds available Pax of 0");
	if (replaceError) {
		return "Unfortunately this date is now fully booked, please select another date";
	}
	return vError;
}

function setCookie(cname, cvalue, exdays) {
	var expires = "";
	if (exdays){	//not passing a days to expire creates a session cookie
		var d = new Date();
		d.setTime(d.getTime() + (exdays*24*60*60*1000));
		expires = "expires="+d.toUTCString();
	}
    //document.cookie = cname + "=" + cvalue + "; " + expires;           
    document.cookie = cname + "=" + encodeURIComponent(cvalue) + "; path=/; " + expires;  //change to enable sessionid cookie to last longer than just the current session.
}

function getCookie(cname) {
    var name = cname + "=";
    var ca = document.cookie.split(';');
    for(var i=0; i<ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1);
        if (c.indexOf(name) == 0) return decodeURIComponent(c.substring(name.length,c.length));
    }
    return "";
}

function ssAjaxRequestAP(ajaxFunction, ajaxData, idTagToPopulate, successFunction){
//if an idTagToPopulate is given it will show content in there
//otherwise it will run the function given with the reponse details passed to it
							
	var processResult = true;
	$.ajax({method: "POST", 
		url: jadeHTTPAP+'&'+ajaxFunction+localTime(),
		data: ajaxData,
		cache: false,
		async: false,
        headers: {
           "cache-control": "no-cache"
        },		
		success: function(data, textStatus, jqXHR){  		
			if (jqXHR.readyState == 4 && jqXHR.status == 200) {	
				checkResultForLogout(data);
				if (successFunction && successFunction != ssJSONResult){
				//don't check this stuff if our success function is already ssJSONResult
					var result = ssJSONResult(data, textStatus, jqXHR);
					processResult = !result || result.result == "OK";
				}
				if (processResult && idTagToPopulate != "") {
					if (jqXHR.responseText.charAt(0) == "{"){
					//we were expecting html but we have json back - probably an error
						ssJSONResult(data, textStatus, jqXHR);
						return;
					} else {
					//populate the tag given
						$("#"+idTagToPopulate).html(jqXHR.responseText);
					}
				}
				if (successFunction && typeof(successFunction) == 'function') {
					successFunction(data, textStatus, jqXHR); 
				}
			} else {
				alertError(jqXHR, textStatus, data);
			}
		},
		error: function(jqXHR, textStatus, errorThrown){
			alertError(jqXHR, textStatus, errorThrown);
		}
	});
}

function ssAjaxRequestGP(ajaxFunction, ajaxData, idTagToPopulate, successFunction){
//if an idTagToPopulate is given it will show content in there
//otherwise it will run the function given with the reponse details passed to it
							
	var processResult = true;
	$.ajax({method: "POST", 
		url: jadeHTTPGP+'&'+ajaxFunction+localTime(),
		data: ajaxData,
		cache: false,
		async: false,
        headers: {
           "cache-control": "no-cache"
        },		
		success: function(data, textStatus, jqXHR){  		
			if (jqXHR.readyState == 4 && jqXHR.status == 200) {	
				checkResultForLogout(data);
				if (successFunction && successFunction != ssJSONResult){
				//don't check this stuff if our success function is already ssJSONResult
					var result = ssJSONResult(data, textStatus, jqXHR);
					processResult = !result || result.result == "OK";
				}
				if (processResult && idTagToPopulate != "") {
					if (jqXHR.responseText.charAt(0) == "{"){
					//we were expecting html but we have json back - probably an error
						ssJSONResult(data, textStatus, jqXHR);
						return;
					} else {
					//populate the tag given
						$("#"+idTagToPopulate).html(jqXHR.responseText);
					}
				}
				if (successFunction && typeof(successFunction) == 'function') {
					successFunction(data, textStatus, jqXHR); 
				}
			} else {
				alertError(jqXHR, textStatus, data);
			}
		},
		error: function(jqXHR, textStatus, errorThrown){
			alertError(jqXHR, textStatus, errorThrown);
		}
	});
}

	</script>
	
	<script>
		function showInfoBar(text, stayUp, warning){
			$("#ID_InfoBarText").html(text);
			if (warning) {
				if (true || stayUp) {
					showModal("Warning", text);
				} else {
					$("#ID_InfoBar").addClass("alert-danger").removeClass("alert-success");
					$("#ID_InfoBar").slideDown();
					if (!stayUp)    //auto hide after 3 seconds
						setTimeout(function(){hideInfoBar()}, 3000);
				}
				
			} else { 
				
						$("#ID_InfoBar").removeClass("alert-danger").addClass("alert-success");
						$("#ID_InfoBar").slideDown();
						if (!stayUp)    //auto hide after 3 seconds
							setTimeout(function(){hideInfoBar()}, 3000);
							
			}
		}

		function hideInfoBar(){
			$("#ID_InfoBar").slideUp();
		}
		
		
		function showPopupInfoBar(text, stayUp, warning, popupId){
			var infoBar = "#ID_PIB_" + popupId;
			var infoBarText = "#ID_PIBText_" + popupId;
			$(infoBarText).html(text);
			if (warning) $(infoBar).addClass("alert-danger").removeClass("alert-success");
			else $(infoBar).removeClass("alert-danger").addClass("alert-success");
			$(infoBar).slideDown();
			if (!stayUp)    //auto hide after 3 seconds
				setTimeout(function(){hidePopupInfoBar(infoBar)}, 3000);
		}
		function hidePopupInfoBar(infoBar){
			$(infoBar).slideUp();
		}

		function showModal(title, body, functionOK){
			//sometimes this function wasn`t loaded yet and causing error, this waits for it to be loaded
			$(document).ready(function () {
				bootBoxModal = bootbox.dialog({
				  message: "<div style='max-height:calc(100vh - 170px);overflow:auto; padding-right:5px;padding-bottom:1rem'><p>" + unescape(body) + "</p></div>",
				  title: title,	
				  closeButton:true,
				  buttons: {
					yes: {
					  label: "Ok",
					  className: "btn--action",
					  callback: functionOK
					}
				  }
				});
			});
		}
		
		function showModalConfirm(title, body, functionYes, functionNo, pLabelYes, pLabelNo){
			var vLabelYes = "Yes";
			var vLabelNo = "No";
			if (pLabelYes) vLabelYes = pLabelYes;
			if (pLabelNo) vLabelNo = pLabelNo;
			//sometimes this function wasn`t loaded yet and causing error, this waits for it to be loaded
			$(document).ready(function () {
				bootBoxModal = bootbox.dialog({
				  message: "<div style='max-height:calc(100vh - 170px);overflow:auto; padding-right:5px;padding-bottom:1rem'><p>" + unescape(body) + "</p></div>",
				  title: title,	
				  closeButton:false,
				  buttons: {
					yes: {
					  label: vLabelYes,
					  className: "btn--std",
					  callback: functionYes
					},
					no: {
					  label: vLabelNo,
					  className: "btn--action",
					  callback: functionNo
					}
				  }
				});
			});
		}
		function showTextOnlyModal(body){
			bootBoxModal = bootbox.dialog({
			  message: "<div style='max-height:calc(100vh - 170px);overflow:auto; padding-right:5px'><p>" + unescape(body) + "</p></div>",
			  closeButton:true
			});
		}
		function showModalTitleText(title, body){
			bootBoxModal = bootbox.dialog({
				message: "<div style='max-height:calc(100vh - 170px);overflow:auto; padding-right:5px'><p>" + unescape(body) + "</p></div>",
				closeButton:true,
				className:"cl_modal",
				title:title,
				onEscape: true,
				backdrop: true
			});
		}
		function showModalConfirmText(title, body, functionToCall, pLabelYes, pLabelNo){
			var vLabelYes = "Yes";
			var vLabelNo = "No";
			if (pLabelYes) vLabelYes = pLabelYes;
			if (pLabelNo) vLabelNo = pLabelNo;
			bootBoxModal = bootbox.prompt({
			  message: "<div style='max-height:calc(100vh - 170px);overflow:auto; padding-right:5px;padding-bottom:1rem'><p>" + unescape(body) + "</p></div>",
			  title: title,
			  inputType: 'textarea',
			  required:true,
			  closeButton:false,
			  swapButtonOrder:true,
			  buttons: { 
				cancel: {
				  label: vLabelNo,
				  className: "btn--std"
				},
				confirm: {
				  label: vLabelYes,
				  className: "btn--action"
				}
				},
			  callback: functionToCall
			  });
		}
		
		function openCart() {
			var submitData = "action=openCart";
			ssAjaxRequest("openCart", submitData, "", ssJSONResult);
		}	

        //ZD#94058
        function doSessionCheck() {
            var submitData = "sessionData=040c8002-209c-48f5-abfb-ab7a85701820";
            ssAjaxRequest("checkSession", submitData, "", ssJSONResult);
			if (sessionTimedout == false) {
				setTimeout(doSessionCheck, 60000);
			}
        }
        //end ZD#94058

		
			jadeHTTP = "/puffingbillyrailways/BookingCat/Availability/?";
			jadeHTTPAP = "/puffingbillyrailways/AgentPortal/Dashboard/?";
			jadeHTTPGP = "/puffingbillyrailways/MemberPortal/Dashboard/?";
			
		var ffTimeoutTimer;
		function ffClickOptionChecked(pKey, pElement) {
			var submitData = "key=" + pKey + "&changeName=" + pElement.name  + "&changeValue=" + pElement.checked;
			ssAjaxRequest("optionChecked", submitData, "", ssJSONResult);
			return false;
		}
		function ffClickOptionValue(pKey, pElement) {
			var submitData = "key=" + pKey + "&changeName=" + pElement.name  + "&changeValue=" + pElement.value;
			ssAjaxRequest("optionValue", submitData, "", ssJSONResult);
			return false;
		}
		var ffChangeValueProcess = false;
		function ffChangeValue(pKey, pElement) {
			if (ffTimeoutTimer) clearTimeout(ffTimeoutTimer);
			if (ffChangeValueProcess) return false;
			ffChangeValueProcess = true;
			var submitData = "key=" + pKey + "&changeName=" + pElement.name  + "&changeValue=" + pElement.value;
			ssAjaxRequest("changeOfValue", submitData, "", ffChangeResponse);
			return false;
		}
		function ffChangeResponse(data, textStatus, jqXHR){
			ffChangeValueProcess = false;
		}
		function ffFlagKeyup(pKey, pElement) {
			if (ffTimeoutTimer) clearTimeout(ffTimeoutTimer);
			ffTimeoutTimer = setTimeout(ffChangeValue, 1500, pKey, pElement);
			return true;
		}
		function ffChangeNameValue(pKey, pName, pValue) {
			var submitData = "key=" + pKey + "&changeName=" + pName  + "&changeValue=" + pValue;
			ssAjaxRequest("changeOfValue", submitData, "", ssJSONResult);
			return false;
		}
		
		function ffNextSubmit(pKey, pName, pValue) {
			if (validatePage()) {
				var submitData = "key=" + pKey + "&changeName=" + pName  + "&changeValue=" + pValue;
				ssAjaxRequest("changeOfValue", submitData, "", ssJSONResult);
			}
			return false;
		}
		
		function ffSubmitFrame(pSubmitAction, pKey, pFormId) {
			if (validatePage()) {
				var submitData = "key=" + pKey;
				if (pFormId) {
				  submitData += "&" + $("#" + pFormId ).serialize();
				}
				ssAjaxRequest(pSubmitAction, submitData, "", ssJSONResult);
			}
		}
		
		function ffScrollBottom(pKey, pElement) {
		    if (pElement.scrollTop + pElement.offsetHeight>= (pElement.scrollHeight - 10) ){
				var submitData = "key=" + pKey + "&changeName=" + pElement.id  + "&changeValue=done";
				ssAjaxRequest("scrolledBottom", submitData, "", ssJSONResult);
			}
			return false;
		}

		function ffPrintForm(div_id){
			$("body").css("visibility", "hidden");
			$("#"+div_id).css("visibility", "visible");
			window.print();
			$("body").css("visibility", "visible");    
		}
	</script>
	
