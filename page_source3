		function SelectProductOpenDated(ProductId, CategoryId){
			var submitData = "ProductId=" + ProductId + "&CategoryId=" + CategoryId ;
			ssAjaxRequest("SelectProductOpenDated", submitData, "", selectProductOpenDatedResult);		
		}
		function selectProductOpenDatedResult(data, textStatus, jqXHR){
			var result = $.parseJSON(data);
			
			if (result.result == "OK"){
			//it worked			
				//location.href = result.url
			} else {
					/*
					if (result.result == "Error") {
						showInfoBar(result.errormessage, true, true);
