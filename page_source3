	<script>
		$( document ).ready(function() {
			var url_string = window.location.href; 
			var url = new URL(url_string);
			var category = url.searchParams.get("category"); //"LAKE";
			var source = "https://apps.customlinc.com.au/puffingbillyrailways/";
			availabilityTable(source, category);
		});
