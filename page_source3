		function getProductFilterHeight() {
		   var productFilterElement = document.querySelector(".product-filtering");
		   
		   	if(productFilterElement == null) {
				return 0;
			}else{
			    var getproductFilterElementStyle = window.getComputedStyle(productFilterElement);

				if (getproductFilterElementStyle.getPropertyValue("position") === "sticky" && parseInt(getproductFilterElementStyle.getPropertyValue("height")) > 0) {
					return parseInt(getproductFilterElementStyle.getPropertyValue("height"));
				} else {
					return 0;
				} 
			}	
		}
		
