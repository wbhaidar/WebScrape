		function actionChangeDateResult(data, textStatus, jqXHR){
			var result = $.parseJSON(data);
			
			if (result.result == "OK"){
			//it worked
				$("#divCategories").html(result.categoryHTML);	
				$("#divPromotions").html(result.promotionHTML);	
				$("#divDateSelector").html(result.availabilityDateHTML);	
				$("#AvailabilityTable").html(result.availabilityHTML);	
				if (typeof availabilityDateChange === "function") { 
					return availabilityDateChange(new Date(changeDateDate));
				}
			} 
		}
		
       function getHeaderHeight() {
            var headerElement = document.querySelector("header");
			if (headerElement == null) {
				return 0;
			}
            var getHeaderElementStyle = window.getComputedStyle(headerElement);

            if (getHeaderElementStyle.getPropertyValue("position") === "fixed" && parseInt(getHeaderElementStyle.getPropertyValue("height")) > 0) {
                return parseInt(getHeaderElementStyle.getPropertyValue("height"));
            } else {
                return 0;
            }           
        }
        
